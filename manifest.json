{
	"name": "HNTP",
	"short_name": "HNTP",
	"version": "1.24.2",
	"manifest_version": 2,
	"browser_specific_settings": {
		"gecko": {
			"id": "{bb9cfae3-9ac6-4c72-b90b-71814ab7d789}",
			"strict_min_version": "56.0"
		}
	},
	"description": "Redesigned new tab page featuring your bookmarks, apps, most visited, and recently closed in a custom layout.",
	"icons": {
		"128": "icon_128.png",
		 "48": "icon_48.png",
		 "16": "icon_16.png"
	},
	"permissions": [
		"bookmarks",
		"topSites",
		"tabs",
		"sessions"
