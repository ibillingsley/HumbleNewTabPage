{
	"name": "HNTP",
	"short_name": "HNTP",
	"version": "1.25",
	"manifest_version": 3,
	"browser_specific_settings": {
		"gecko": {
			"id": "{bb9cfae3-9ac6-4c72-b90b-71814ab7d789}",
			"strict_min_version": "108.0"
		}
	},
	"description": "Redesigned new tab page featuring your bookmarks, apps, most visited, and recently closed in a custom layout.",
	"icons": {
		"128": "icons/icon_128.png",
		"48": "icons/icon_48.png",
		"16": "icons/icon_16.png"
	},
	"permissions": [
		"bookmarks",
		"topSites",
		"tabs",
		"sessions"
	],
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"chrome_settings_overrides": {
		"homepage": "newtab.html"
	},
	"options_ui": {
		"page": "newtab.html?options",
		"open_in_tab": true
	}
}
