	"chrome_url_overrides":
	{
		"newtab": "newtab.html"
	},
  "chrome_settings_overrides":
	{
		"homepage": "newtab.html"
	},
	"options_ui": {
		"page": "newtab.html?options",
		"chrome_style": false,
		"open_in_tab": true
	},
	"web_accessible_resources": [
		"newtab.html"
	]
}